SELECT name FROM Banned_processes UNION SELECT name FROM Processes;
SELECT * FROM Processes WHERE name LIKE 'p%';
SELECT * FROM Processes WHERE name IN (SELECT name FROM Banned_processes WHERE user_id=3);

DROP FUNCTION IF EXISTS myFun;
DELIMITER //
CREATE FUNCTION userName (user int) 
RETURNS varchar(25)
BEGIN 
	DECLARE result varchar(25);
	SELECT name into result FROM Users WHERE id=user;
	RETURN result;
	
END //
DELIMITER ;


